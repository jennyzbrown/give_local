
<div class="col-md-6 col-md-offset-6">
  <div class="row">
    <div class="panel-heading">
      <ul class="pricing-table">
        <li class="title">My Account</li>
        <li class="price"><%= @user.user_name %></li>
        <li class="price"><%= @user.email %></li>
        <li class="price">Annual Goal: <%= @user.goal %></li>
        <li class="price">Account: <%= @user.account %></li>
        <li class="cta-button"><%= link_to 'Edit My Account', edit_user_registration_path, class: 'button round' %></li>
        <li class="cta-button"><%= link_to "Delete My Account", user_registration_path, method: :delete, data: { confirm: 'Are you sure?' }, class: 'button round' %></li>

      </ul>
    </div>
  </div>
</div>
